        <item>77.3881426005</item>    <!--  55 - 90  -->
        <item>90.0748914709</item>    <!--  90 - 148  -->
        <item>106.099757597</item>    <!--  148 - 245  -->
        <item>126.784839595</item>    <!--  245 - 403  -->
        <item>154.858709208</item>    <!--  403 - 665  -->
